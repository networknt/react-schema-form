{
    "schema": {
        "type": "object",
        "title": "Read Only Test",
        "required": ["name"],
        "properties": {
            "name": {
                "title": "Name",
                "type": "string"
            },
            "password": {
                "title": "Password",
                "type": "string",
                "readonly": true
            },
            "text": {
                "title": "Text",
                "type": "string",
                "readonly": true
            },
            "textarea": {
                "title": "TextArea",
                "type": "string",
                "readonly": true
            },
            "number": {
                "title": "Number",
                "type": "number",
                "readonly": true
            },
            "rememberMe": {
                "title": "Remember me",
                "type": "boolean",
                "default": false,
                "readonly": true
            },
            "radios": {
                "title": "Basic radio button example",
                "type": "string",
                "readonly": true,
                "enum": [
                    "a",
                    "b",
                    "c"
                ]
            },
            "environment": {
                "type": "string",
                "title": "Environment",
                "readonly":true,
                "enum": [
                    "LOCAL",
                    "SIT1",
                    "SIT2",
                    "SIT3",
                    "UAT1",
                    "UAT2"
                ]
            },
            "date": {
                "title": "Date",
                "type": "date"
            }
        }
    },
    "form": [
        "name",
        {
            "key": "password",
            "type": "password"
        },
        "text",
        {
            "key": "textarea",
            "type": "textarea",
            "rows": 10
        },
        "number",
        "rememberMe",
        {
            "key": "radios",
            "type": "radios",
            "titleMap": [
                {
                    "value": "c",
                    "name": "C"
                },
                {
                    "value": "b",
                    "name": "B"
                },
                {
                    "value": "a",
                    "name": "A"
                }
            ]
        },
        "environment",
        {
            "key": "date",
            "type": "date"
        }

    ]
}
